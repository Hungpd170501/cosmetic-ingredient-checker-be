{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "cosmetic-ingredient-checker-be",
      "image": "hungpd170501/cosmetic-ingredient-checker-be:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "dev"
        }
      ]
    }
  ]
}